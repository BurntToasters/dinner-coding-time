public class SlideKarel extends Karel
{
    private void turnRight()
  {
     turnLeft();
     turnLeft();
     turnLeft();
  }
  public void run()
 {
    putBall();
    move();
    turnRight();
    move();
    putBall();
    move();
    turnLeft();
    move();
    putBall();
 }
   
    
}